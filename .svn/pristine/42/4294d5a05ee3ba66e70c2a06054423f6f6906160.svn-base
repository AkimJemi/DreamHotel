<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>roomList.jsp</title>
<style type="text/css">
.main_container {
	display: flex;
	justify-content: center;
	align-items: center;
	flex-wrap: wrap;
	width: 100%;
	height: 800px
}

.main_container>* {
	border: 2px solid black;
}

.main_subContainer_A {
	height: 30%;
	width: 90%;
}

.main_subContainer_B_1 {
	display: flex; height : 40%;
	width: 70%;
	height: 40%;
}

.main_subContainer_B_2 {
	height: 40%;
	width: 20%;
}

.main_subContainer_C {
	height: 30%;
	width: 90%;
}
</style>
</head>
<body>
	<%@ include file="../../include/header.jsp"%>
	<div class="main_container">
		<div class="main_subContainer_A">a</div>
		<div class="main_subContainer_B_1">
			<c:forEach var="rooms" items="${rooms}">
				<div>
					<div>${rooms.no }</div>
					<div>${rooms.room_num }</div>
					<div>${rooms.room_title }</div>
					<div>${rooms.max }</div>
					<div>${rooms.adult_cost }</div>
					<div>${rooms.child_cost }</div>
					<div>${rooms.explanation }</div>
					<div>${rooms.images }</div>
					<div>${rooms.color_code }</div>
					<div>${rooms.delete_flag }</div>
					<div>${rooms.created_at }</div>
					<div>${rooms.delete_flag }</div>
					<div>${rooms.updated_at }</div>
				</div>
			</c:forEach>
		</div>
		<div class="main_subContainer_B_2">s</div>
		<div class="main_subContainer_C">b</div>
	</div>
</body>
</html>